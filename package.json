{
  "name": "dokploy-dns-manager",
  "version": "1.0.0",
  "description": "Automatic DNS management for Dokploy containers with Simply.com",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build-frontend": "cd frontend && npm run build && cp -r build/* ../public/",
    "install-frontend": "cd frontend && npm install"
  },
  "keywords": [
    "dokploy",
    "dns",
    "docker",
    "traefik",
    "simply.com",
    "automation"
  ],
  "author": "Dokploy DNS Manager",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "dockerode": "^4.0.2",
    "axios": "^1.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}